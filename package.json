  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "htmx.org": "^2.0.3",
    "parcel": "^2.12.0",
    "sass": "1.77.6",
    "sortablejs": "^1.15.2"
  },
  "config": {
    "assets_src": "./assets",
    "assets_dist": "./static"
